<div id="<%= dom_id expense %>">
  <div>
    <strong>Spent on:</strong>
    <%= expense.spent_on %>
  </div>

  <div>
    <strong>Amount:</strong>
    <%= expense.amount %>
  </div>

  <div>
    <strong>Description:</strong>
    <%= expense.description %>
  </div>

  <div>
    <strong>Category:</strong>
    <%= expense.category_id %>
  </div>

  <div>
    <strong>User:</strong>
    <%= expense.user_id %>
  </div>

</div>
